
<!-- BEGIN LINK LIST -->
<form name="link_list" action="" method="post">
	<table class="design" cellspacing="0">
		<tr>
			<th>Links</th>
			<th>Status</th>
			<th>Verwijderen</th>
		</tr>
		<!-- BEGIN LINKS -->
		<!-- BEGIN CAT_DIV -->
		<tr>
			<th colspan="3">{cat_heading}</th>
		</tr>
		<!-- END CAT_DIV -->
		<tr class="{row}">
			<td><a href="?id={link_id}">{link_heading}</a></td>
			<td>{active}</td>
			<td><input type="checkbox" name="del_link[]" value="{link_id}" /></td>
		</tr>
		<!-- END LINKS -->	
	</table>
	<p><input type="submit" name="delete" value="Verwijder" /></p>
</form>
<!-- END LINK LIST -->
