<p class="attention">{message}</p>

<form name="article_list" id="article_list" action="" method="post">
<table class="design" cellspacing="0">
	<tr>
		<th>Heading / category</th>
		<th>Date inserted</th>
		<th>Date modified</th>
		<th>Date expire</th>
		<th>Active</th>
		<th>Delete</th>
	</tr>
	<!-- BEGIN ITEMS -->
	<!-- BEGIN CAT_DIV -->
	<tr>
		<th colspan="6">{cat_heading}</th>
	</tr>
	<!-- END CAT_DIV -->	
	<tr class="{row}{expired}">
		<td><a href="article_edit.php?article_id={art_id}" title="Edit this article"><strong>{art_heading}</strong></a></td>
		<td class="date">{art_date_insert}</td>
		<td class="date">{art_date_mod}</td>
		<td class="date">{art_date_end}</td>
		<td>{art_active}</td>
		<td><input type="checkbox" name="del_article[]" value="{art_id}" /></td>
	</tr>
	<!-- END ITEMS -->
</table>
<p><input type="submit" name="delete" value="Delete" /></p>
</form>
